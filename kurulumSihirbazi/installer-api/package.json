{
  "name": "qodify-installer-api",
  "version": "1.0.0",
  "description": "Qodify Installer Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init_control_plane",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:create:control": "node scripts/create-control-db.js qodifynet"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "socket.io": "^4.8.1",
    "pg": "^8.13.1",
    "child_process": "^1.0.2",
    "fs-extra": "^11.2.0",
    "dotenv": "^16.4.7",
    "zod": "^3.24.1",
    "dns": "^0.2.2",
    "pm2": "^5.4.3",
    "adm-zip": "^0.5.16",
    "uuid": "^11.0.4",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.4.0",
    "cookie-parser": "^1.4.7",
    "@prisma/client": "^5.19.1",
    "argon2": "^0.41.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/fs-extra": "^11.0.4",
    "@types/adm-zip": "^0.5.6",
    "@types/uuid": "^10.0.0",
    "@types/multer": "^1.4.12",
    "typescript": "^5.7.3",
    "ts-node-dev": "^2.0.0",
    "@typescript-eslint/parser": "^8.19.1",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "eslint": "^9.18.0",
    "prisma": "^5.19.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.6"
  }
}
